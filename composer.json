{
  "name": "itech-world/sulu-article-twig-extension-filter-bundle",
  "description": "SuluArticleTwigExtensionFilterBundle extends the Sulu CMS to enable article retrieval in TWIG without ElasticSearch",
  "type": "sulu-bundle",
  "license": "MIT",
  "version": "0.1.0",
  "keywords": [
    "sulu",
    "symfony",
    "cms",
    "content-management-system",
    "php",
    "article"
  ],
  "authors": [
    {
      "name": "Steeven THOMAS",
      "homepage": "https://github.com/steeven-th"
    }
  ],
  "require": {
    "php": "^8.2",
    "sulu/sulu": "^3.0@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "ItechWorld\\SuluArticleTwigExtensionFilterBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ItechWorld\\SuluArticleTwigExtensionFilterBundle\\": "src/tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "phpunit": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true
    }
  }
}
